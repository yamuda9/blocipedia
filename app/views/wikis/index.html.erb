<h1>Wikis</h1>

<div class="row">
  <div class="col-md-4">
    <%= link_to "Add New Wiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
  <br />
  <div class="col-md-8">
    <% @wikis.each do |w| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to w.title, w %>
          </h4>
          <small>
            <% w.body %>
          </small>
        </div>
      </div>
    <% end %>
  </div>
</div>
